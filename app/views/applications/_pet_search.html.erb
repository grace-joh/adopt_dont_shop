<p>Add a Pet to this Application</p>
<%= form_with url: "/applications/#{@application.id}", method: :get, local: true do |f| %>
  <%= f.label :search_pet_name %>
  <%= f.text_field :search_pet_name%>
  <%= f.submit "Search" %>
<% end %>

<% @pets.each do |pet| %>
  <%= link_to "#{pet.name}", "/pets/#{pet.id}" %>
  <%= button_to "Adopt me. I'm lovable!", params = { pet_id: pet.id }, method: "patch" %>
<% end %>