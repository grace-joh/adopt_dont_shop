<h1>New Application</h1>
<%= form_with url: "/applications/new", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <br>
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.submit 'Submit' %>
<% end %>
